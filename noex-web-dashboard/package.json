{
  "name": "noex-web-dashboard",
  "version": "0.1.0",
  "description": "Web-based dashboard for monitoring noex applications",
  "type": "module",
  "bin": {
    "noex-web-dashboard": "./dist/bin/noex-web-dashboard.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch bin/noex-web-dashboard.ts -- --port 9876 --web-port 3001",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.node.json",
    "start": "node dist/bin/noex-web-dashboard.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.2",
    "svelte": "^5.16.2",
    "svelte-check": "^4.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  },
  "peerDependencies": {
    "noex": ">=0.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "noex",
    "dashboard",
    "monitoring",
    "svelte",
    "web"
  ],
  "author": "",
  "license": "MIT"
}
